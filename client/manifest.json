{
    "manifest_version": 3,
    "name": "CodePad",
    "version": "0.1.3",
    "description": "notepad records highlighted text",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        "config.json"
    ],
    "permissions": [
        "tabs",
        "activeTab"
    ]
}
